<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.ctw.spider.dao.mapper.MovieCommentMapper">

	<select id="queryMovieCommentByMid" parameterType="string" resultType="movieComment">
		select * from movieComment where mid=#{mid}
	</select>	
	
	<select id="queryMovieCommentByUserCode" parameterType="string" resultType="movieComment">
		select * from movieComment where userCode=#{userCode}
	</select>	
</mapper>